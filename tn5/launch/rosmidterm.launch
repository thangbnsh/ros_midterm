<launch>
  <!-- Load Gazebo -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch" />

  <!-- Spawn Robot Model -->
  <node name="spawn_model" pkg="gazebo_ros" type="spawn_model"
        args="-file $(find tn5)/urdf/tn5.urdf -urdf -model tn5"
        output="screen" />

  <!-- Fake Joint Calibration -->
  <node name="fake_joint_calibration" pkg="rostopic" type="rostopic"
        args="pub /calibrated std_msgs/Bool true" />

  <!-- Load robot URDF -->
  <param name="robot_description" command="$(find xacro)/xacro $(find tn5)/urdf/tn5.urdf" />

  <!-- Node xuất thông tin trạng thái khớp -->
  <node name="joint_state_publisher_gui" pkg="joint_state_publisher_gui" type="joint_state_publisher_gui" />

  <!-- Node xuất trạng thái robot -->
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" />

  <!-- RViz hiển thị robot và cảm biến -->
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find tn5)/rviz/configrviz.rviz" output="screen" />
</launch>

